<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <A6oModuleRoot>$(SolutionDir)..\..\..\..\</A6oModuleRoot>
    <A6oOut>$(A6oModuleRoot)..\OUT\$(Platform)\$(Configuration)\</A6oOut>
    <A6oMdkRoot>$(A6oOut)</A6oMdkRoot>
    <A6oMdkInclude>$(A6oMdkRoot)include</A6oMdkInclude>
    <A6oMdkLib>$(A6oMdkRoot)</A6oMdkLib>
    <A6oMdkLibraries>Armadito.lib</A6oMdkLibraries>
    <LibclamavRoot>$(SolutionDir)..\..\deps\libclamav\$(Platform)\$(Configuration)\</LibclamavRoot>
    <LibclamavInclude>$(LibclamavRoot)include</LibclamavInclude>
    <LibclamavLib>$(LibclamavRoot)lib\</LibclamavLib>
    <OpensslRoot>$(SolutionDir)..\..\deps\openssl\$(Platform)\$(Configuration)\</OpensslRoot>
    <OpensslInclude>$(OpensslRoot)include</OpensslInclude>
    <OpensslLib>$(OpensslRoot)lib\</OpensslLib>
    <A6oModuleAdditionalInclude>$(LibclamavInclude);$(OpensslInclude)</A6oModuleAdditionalInclude>
    <A6oModuleAdditionalLib>$(LibclamavLib)</A6oModuleAdditionalLib>
    <A6oModuleAdditionalLibraries>libclamav.lib</A6oModuleAdditionalLibraries>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="A6oModuleRoot">
      <Value>$(A6oModuleRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oOut">
      <Value>$(A6oOut)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oMdkRoot">
      <Value>$(A6oMdkRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oMdkInclude">
      <Value>$(A6oMdkInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oMdkLib">
      <Value>$(A6oMdkLib)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oMdkLibraries">
      <Value>$(A6oMdkLibraries)</Value>
    </BuildMacro>
    <BuildMacro Include="LibclamavRoot">
      <Value>$(LibclamavRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="LibclamavInclude">
      <Value>$(LibclamavInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="LibclamavLib">
      <Value>$(LibclamavLib)</Value>
    </BuildMacro>
    <BuildMacro Include="OpensslRoot">
      <Value>$(OpensslRoot)</Value>
    </BuildMacro>
    <BuildMacro Include="OpensslInclude">
      <Value>$(OpensslInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="OpensslLib">
      <Value>$(OpensslLib)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oModuleAdditionalInclude">
      <Value>$(A6oModuleAdditionalInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oModuleAdditionalLib">
      <Value>$(A6oModuleAdditionalLib)</Value>
    </BuildMacro>
    <BuildMacro Include="A6oModuleAdditionalLibraries">
      <Value>$(A6oModuleAdditionalLibraries)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>